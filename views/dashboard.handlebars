<br>
<div class="container">
    <div class="row">
        <div class="col">
            <h1 class="text-center">Your Dashboard</h1>
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{#each blogs as |blog|}}
            <div class="card">
                <div class="card-body">
                    <h1 class="card-title">{{blog.title}}</h1>
                    <p>Posted by on </p>
                    <hr>
                    <p class="card-text"></p>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>